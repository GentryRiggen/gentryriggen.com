{
  "name": "raysheahomes",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js --mode=development",
    "prebuild": "rimraf build/",
    "build": "webpack --mode=production",
    "lint": "tslint",
    "test": "jest",
    "test:e2e": "start-test start 3001 cypress:run",
    "test:pre-commit": "CI=true yarn test --findRelatedTests",
    "cypress:run": "cypress run",
    "predeploy": "yarn build",
    "deploy": "firebase deploy",
    "preserve": "yarn build",
    "serve": "firebase serve --only hosting"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint",
      "yarn test:pre-commit"
    ]
  },
  "jest": {
    "setupFiles": [
      "./src/setupTests.ts"
    ],
    "testRegex": "(^src.*/__tests__/.*|(\\.|/)(test))\\.(ts|tsx|js|jsx)$",
    "collectCoverageFrom": [
      "src/domains/**/utils/*.{ts,tsx,js}",
      "src/domains/**/selectors/*.{ts,tsx,js}",
      "src/domains/**/workflows/*.{ts,tsx,js}",
      "src/lib/**/utils/*.{ts,tsx,js}"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/src/test/",
      "<rootDir>/build/",
      "<rootDir>/cypress/",
      "<rootDir>/public/"
    ],
    "moduleDirectories": [
      "./node_modules",
      "./src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    }
  },
  "dependencies": {
    "@chakra-ui/core": "^0.8.0",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "babel-polyfill": "^6.26.0",
    "emotion-theming": "^10.0.27",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-icons": "^3.10.0",
    "react-router-dom": "^5.2.0",
    "react-toggle": "^4.1.1"
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.10.5",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@types/jest": "26.0.24",
    "@types/webpack": "5.28.0",
    "babel-loader": "8.1.0",
    "copy-webpack-plugin": "6.4.1",
    "css-loader": "4.0.0",
    "cypress": "4.11.0",
    "firebase": "7.17.1",
    "firebase-tools": "8.6.0",
    "fork-ts-checker-webpack-plugin": "5.0.11",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "rimraf": "3.0.2",
    "source-map-loader": "1.0.1",
    "start-server-and-test": "1.11.2",
    "style-loader": "1.2.1",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-standard": "9.0.0",
    "tslint-react": "5.0.0",
    "typescript": "3.9.10",
    "webpack": "5.69.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  }
}
